{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "App",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "runtimeExecutable": "npm",
      "type": "node",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules --no-warnings=ExperimentalWarning"
      },
      "request": "launch",
      "name": "All tests",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--runInBand"
      ],
      "enableContentValidation": false,
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules --no-warnings=ExperimentalWarning"
      },
      "request": "launch",
      "name": "Current test file",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "${fileBasenameNoExtension}",
        "--runInBand"
      ],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "node",
      "env": {
        "NODE_OPTIONS": "--experimental-vm-modules --no-warnings=ExperimentalWarning"
      },
      "request": "launch",
      "name": "vscode-jest-tests.v2",
      "program": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--runInBand",
        "--watchAll=false",
        "--testNamePattern",
        "${jest.testNamePattern}",
        "--runTestsByPath",
        "${jest.testFile}"
      ],
      "console": "integratedTerminal",
      "sourceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "internalConsoleOptions": "neverOpen"
    }
  ]
}